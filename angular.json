{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular": {
      "root": "projects/angular",
      "sourceRoot": "projects/angular",
      "projectType": "library",
      "prefix": "ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/angular/tsconfig.lib.json",
            "project": "projects/angular/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular/test.ts",
            "tsConfig": "projects/angular/tsconfig.spec.json",
            "karmaConfig": "projects/angular/karma.conf.js",
            "codeCoverageExclude": [
              "**/test/**/*",
              "**/testing/**/*",
              "**/internal/**/*"
            ],
            "styles": [
              "test.theme.scss"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/angular/tsconfig.lib.json",
              "projects/angular/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "test-shell": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/test-shell",
      "sourceRoot": "projects/test-shell/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/test-shell",
            "index": "projects/test-shell/src/index.html",
            "main": "projects/test-shell/src/main.ts",
            "polyfills": "projects/test-shell/src/polyfills.ts",
            "tsConfig": "projects/test-shell/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/test-shell/src/favicon.ico",
              "projects/test-shell/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/test-shell/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/test-shell/src/environments/environment.ts",
                  "with": "projects/test-shell/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "test-shell:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "test-shell:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test-shell:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/test-shell/src/test.ts",
            "polyfills": "projects/test-shell/src/polyfills.ts",
            "tsConfig": "projects/test-shell/tsconfig.spec.json",
            "karmaConfig": "projects/test-shell/karma.conf.js",
            "assets": [
              "projects/test-shell/src/favicon.ico",
              "projects/test-shell/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/test-shell/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/test-shell/tsconfig.app.json",
              "projects/test-shell/tsconfig.spec.json",
              "projects/test-shell/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/test-shell/e2e/protractor.conf.js",
            "devServerTarget": "test-shell:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test-shell:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "test-shell"
}